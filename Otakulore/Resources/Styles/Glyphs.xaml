<?xml version="1.0" encoding="utf-8"?>
<?xaml-comp compile="true"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <x:String x:FieldModifier="private" x:Key="GlyphFontFamily">SegoeAssets</x:String>

    <FontImageSource x:Key="HomeGlyph" FontFamily="{StaticResource GlyphFontFamily}" Glyph="&#xE80F;"/>
    <FontImageSource x:Key="SettingsGlyph" FontFamily="{StaticResource GlyphFontFamily}" Glyph="&#xE713;"/>
    <FontImageSource x:Key="BackGlyph" FontFamily="{StaticResource GlyphFontFamily}" Glyph="&#xE72B;"/>
    <FontImageSource x:Key="FilterGlyph" FontFamily="{StaticResource GlyphFontFamily}" Glyph="&#xE71C;"/>
    <FontImageSource x:Key="CalendarGlyph" FontFamily="{StaticResource GlyphFontFamily}" Glyph="&#xE787;"/>

</ResourceDictionary>