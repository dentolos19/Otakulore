<Page x:Class="Otakulore.Views.SchedulesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
    <TabView IsAddTabButtonVisible="False">
        <TabView.TabItems>
            <TabViewItem Header="Winter" IsClosable="False"/>
            <TabViewItem Header="Spring" IsClosable="False"/>
            <TabViewItem Header="Summer" IsClosable="False"/>
            <TabViewItem Header="Fall" IsClosable="False"/>
        </TabView.TabItems>
    </TabView>
</Page>